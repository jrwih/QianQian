<!--pages/index/index.wxml-->
<view class="page">
  <view class="page_hd">
    <swiper autoplay="1" indicator-dots="1" circular="1" style="height: 480rpx;">
      <block wx:for="{{swiperList}}" wx:key="*this" wx:for-index="imgIndex">
        <swiper-item>
          <image src="{{item.url}}" mode="aspectFit" style="width: 100%;" />
        </swiper-item>
      </block>
    </swiper>
  </view>
  <view class="page_bd">
  </view>
</view>